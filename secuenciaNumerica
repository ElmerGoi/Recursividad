public class secuenciaNumerica{
    
    public static void main (String[] args) {
        secuenciaNumerica rogelio = new secuenciaNumerica();
        System.out.println("la secuencia numerica es de " + rogelio.f(5));
    }   
    public int f (int n){
        if(n==0 || n==1) {
            return 1;
        } else {
            return 3*f(n-2) + 2*f(n-1);
        
        }
    }
}
